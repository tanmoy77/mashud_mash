*{
	margin:0;
	padding:0;
	font-family: right;
	box-sizing: border-box;
	
}
.container {
	align-items: center;
	font-family: center;
	padding: flex;
}
.pagination {
	display:flex;
	align-items: center;
	background: #fff;
	color:#383838;
	padding: 10px 40px;
	border-radius: 6px;
}
ul{
	margin: 20px 30px;
}
ul li{
	display: inline-block;
	margin: 0 10px;
	--background: #ccc;
	height:45px;
	width: 45px;
	border-radius: 50%;
	text-align: center;
	font-size: 22px;
	line-height: 45px;
	font-weight: 500;
	cursor: pointer;
	background-position: 0 -45px;
	transition: background-position 0.5s;
}
ul li.active {
	color: #fff;
	background-image: linear-gradient(#ff4568, #ff4568);
	background-repeat: no-repeat;
	background-position: 0 0;
}
.btn1, .btn2 {
	display: line-flex;
	align-items: center;
	font-size: 22px;
	font-weight: 500;
	color: #383838;
	background: transparent;
	outline: none;
	border: none;
	cursor: pointer;
}
.btn1 img{
	width:24px;
	margin-right; 15px;
}
.btn2 img{
	width:24px;
	margin-left: 15px;
	transform: rotate(180deg);
	
}